{
  "author": "Stephen Blankenship",
  "name": "notes",
  "description": "a node.js version of Rails' \"rake notes\" functionality",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "git://github.com/stephenb/node-notes.git"
  },
  "main": "./lib/notes",
  "scripts": {
    "build": "cake build",
    "build:watch": "nodemon -q -w src -e js,coffee -x npm run -s build"
  },
  "engines": {
    "node": ">= v0.4.7"
  },
  "contributors": [
    { "name": "Veselin Todorov",
      "email": "hi@vesln.com" }
    ],
  "dependencies": {
    "colors":  "*"
  },
  "devDependencies": {
    "coffee-script": ">= 1.1.1"
  },
  "bin": {
    "notes": "./bin/notes"
  },
  "keywords": [
    "notes",
    "annotate",
    "todo",
    "fixme"
  ],
  "preferGlobal": "true"
}
